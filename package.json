{
  "name": "@massimo-cassandro/auto-datatables-bs5",
  "version": "2.2.1",
  "description": "jQuery datatables utilities (bootstrap 5)",
  "author": "Massimo Cassandro",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "datatable"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massimo-cassandro/auto-datatables-bs5.git#main"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "UPD-version": "npx update-version",
    "upd @m": "npx upd@m",
    "npm-publish": "npm publish",
    "demo rollup": "npx rollup --config ./demo/rollup.config.mjs --watch",
    "demo sass": "npx sass ./demo/demo.scss:./demo/demo.css --load-path=./node_modules/ --style=expanded --watch",
    "demo server": "python3 -m http.server"
  },
  "browserslist": [
    "last 1 version",
    "iOS >= 13",
    "Safari >= 12",
    "not ie <= 11",
    "> 3% in IT"
  ],
  "stylelint": {
    "extends": "@massimo-cassandro/linters-config/stylelintrc.js"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@massimo-cassandro/linters-config/eslintrc.js"
    ],
    "ignorePatterns": []
  },
  "files": [
    "src/**/*.{js,scss,css,map}",
    "index.js"
  ],
  "devDependencies": {
    "@massimo-cassandro/dev-utilities": "^1.18.1",
    "@massimo-cassandro/linters-config": "^1.5.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "bootstrap": "^5.2.3",
    "rollup": "^3.20.0",
    "sass": "^1.59.3"
  },
  "dependencies": {
    "@massimo-cassandro/js-utilities": "^1.6.1",
    "mustache": "^4.2.0"
  }
}
