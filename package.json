    "@stylistic/stylelint-plugin": "^4.0.0",
    "bootstrap": "^5.2.3",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "rollup": "^3.20.0",
    "sass": "^1.59.3",
